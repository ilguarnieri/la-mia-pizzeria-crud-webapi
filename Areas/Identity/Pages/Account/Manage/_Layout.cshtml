@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout;
    }
    else
    {
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}


<main id="dashboard" class="dashboard-background welcome-wrapper d-flex align-items-center h-100 w-100 justify-content-center text-black overflow-hidden">

    <section class="dashboard-wrapper container h-100">
        
        <div class="pt-3">
            <h2>Dashboard</h2>
            <hr />

            <div class="row">

                <div class="col-md-3">
                    <partial name="_ManageNav" />
                </div>

                <div class="col-md-9 h-100">
                    @RenderBody()
                </div>

            </div>
        </div>       
    </section>
</main>



@section Scripts {
    @RenderSection("Scripts", required: false)
}
